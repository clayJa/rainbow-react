{
  "name": "rainbow-react",
  "version": "0.0.2",
  "description": "a simple react UI component",
  "main": "dist/lib/index",
  "types": "dist/lib/index",
  "files": [
    "/dist/**/*"
  ],
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --config=jest.config.js --runInBand",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "ci": "cross-env NODE_ENV=test JEST_JUNIT_OUTPUT=./test-results/jest/results.xml jest --config=jest.config.ci.js",
    "doc": "cross-env NODE_ENV=production webpack --config webpack.config.doc.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clayJa/rainbow-react.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/clayJa/rainbow-react/issues"
  },
  "homepage": "https://github.com/clayJa/rainbow-react#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.3",
    "@types/react-test-renderer": "^16.8.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^24.7.1",
    "babel-preset-react-app": "^9.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.1",
    "jest-junit": "^6.4.0",
    "node-sass": "^7.0.0",
    "prism-react-renderer": "^0.1.6",
    "raw-loader": "^3.0.0",
    "react-test-renderer": "^16.8.6",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "svg-sprite-loader": "^4.1.6",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.3",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0"
  }
}
